<div class="one-col">
<h1>ログイン</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>

  <%= f.label :"メールアドレス" %>
  <%= f.email_field :email %>

  <%= f.label :"パスワード" %>
  <%= f.password_field :password %>

  <p class="utility-btn"><%= f.submit "ログイン" %></p>
<% end %>
</div>